{
    "version": 2,
    "builds": [
      {
        "src": "src/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/api/(.*)",
          "dest": "src/app.js"
        },
        {
          "src": "/(.*)",
          "dest": "src/$1"
        }
      ]
      
  }
  